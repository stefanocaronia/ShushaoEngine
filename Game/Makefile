define PRELINKCMDS
	@echo Running prelink commands
	@IF EXIST res\ (xcopy /Q /E /Y /I res "..\bin\Debug-windows-x86_64\Game\" > nul) ELSE (xcopy /Q /Y /I res "..\bin\Debug-windows-x86_64\Game\" > nul)
endef

